@import url("src/style/variable.pcss");
@import url("src/style/clear.pcss");
@import url("src/style/mixin.pcss");

#app {
    width: 100%;
    height: 100%;
}

.attache-icon {
    @mixin icon-l;

    background: url("/static/images/attache.svg");
}

.send-icon {
    @mixin icon-m;

    background: url("/static/images/send.svg");
}

.menu-icon {
    @mixin icon-s;

    background: url("/static/images/menu.svg");
}
