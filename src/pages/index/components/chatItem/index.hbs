
<li class="{{ styles.root }}">

    <div class="{{ styles.root__avatar }}">
        {{{components_avatar imageUrl=item.avatar diameter='S'
            avatarOwner=item.display_name }}}
    </div>

    <div class="{{ styles.root__label }}">
        {{item.display_name}}
    </div>

    <div class="{{ styles.root__date }}">
        {{item.date}}
    </div>
</li>
