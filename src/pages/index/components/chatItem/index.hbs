
<li class="{{ styles.root }}">

    <div class="{{ styles.root }}__avatar">
        {{{components_avatar imageUrl=item.avatar diameter='S'
            avatarOwner=item.display_name }}}
    </div>

    <div class="{{ styles.root }}__label">
        {{item.display_name}}
    </div>

    <div class="{{ styles.root }}__date">
        {{item.date}}
    </div>
</li>
